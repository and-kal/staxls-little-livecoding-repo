$ every 8 (slice 4 "[3 2 1 0]*2")
$ every 12 ((hurry 2) . (|+ n (irand 8)) . (# cut 1))
$ whenmod 14 11 (linger (1/5))
$ whenmod 7 5 ((hurry 2) . (slow 2)) $ never (# accelerate ("[-2 2]")) 
$ stack [
    "{[galvaKicks:3!2 galvaClaps:2!3], galvaPerc:3*10}%1" 
