d3 
    $ 
    whenmod 12 9 (hurry (slow 6 $ range 1 1.05 $ saw)) $ 
    stack [
        somecyclesBy 0.5 (0.125 <~) $ s "[KILPACK_HATS:1*8, KILPACK_SNARES:8*16?]" # cut 0 # up (-2)
        , somecyclesBy 0.5 (mask "~!2 t!1") $ sometimes (randslice 16) $ slice 8 "[2 3 4 6] [1 0 4 0]" $ s "GRIME-Eski:30*6" # cut 2 # up "<12 7 2>"
