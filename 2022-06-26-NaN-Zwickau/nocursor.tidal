$ stack [
    whenmod 20 18 ((|+ n ((range 1 200 $ rand))) . (# cut 1)) $ rarely ((jux rev)) $ s "Mega_Genesis:13([3|5],8)" *| up (scale "sikah" "[[0|12] [2|4] 3 7]*4") # cut 0 
