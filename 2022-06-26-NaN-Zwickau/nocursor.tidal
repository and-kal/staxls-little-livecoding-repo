    someCyclesBy (1/8) (|+ n (irand 5)) $
    every 4 (# up "[7|-7|2]") $
    s "umru_sounds:12*5" # cut 1 
    ,
    rarely (# up ("[7|-2]")) $ s "[~!2 umru_vocal:4*2 ~!2]" # cut 2 # gain 1 
    ]
